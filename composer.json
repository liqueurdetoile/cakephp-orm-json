{
    "name": "liqueurdetoile/cakephp-orm-json",
    "description": "Cakephp plugin to provide easy control over JSON type fields in database",
    "type": "cakephp-plugin",
    "license": "MIT",
    "version": "2.0.0-dev",
    "authors": [
        {
            "name": "Liqueur de Toile",
            "homepage": "https://liqueurdetoile.com"
        }
    ],
    "keywords": [
      "database",
      "orm",
      "json",
      "json fields",
      "json data",
      "json_extract",
      "mysql",
      "behavior",
      "table",
      "entity",
      "trait",
      "select",
      "order",
      "sort",
      "where",
      "filter"
    ],
    "require": {
        "php": "^7.1",
        "cakephp/cakephp": "^3.5|^4",
        "adbario/php-dot-notation": "^2.0",
        "mustache/mustache": "^2.13"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "phpunit/phpunit": "^5.7.14|^6.0",
        "phpunit/php-code-coverage": "5.3",
        "phpstan/phpstan": "^0.12",
        "raul338/cakephp-phpstan-extensions": "^2.1",
        "phpstan/extension-installer": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Lqdt\\OrmJson\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lqdt\\OrmJson\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "scripts": {
        "test": "phpunit && phpstan analyze --memory-limit 1G",
        "doc": "php phpDocumentor.phar -d ./src -t ./docs",
        "stan": "phpstan analyze --memory-limit 1G"
    }
}
